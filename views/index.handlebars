<div class="row">
  <div class="col s6">
     <div class="transparent card-panel">
      <h4><strong class="black-text text-darken-4 flow-text"style="font-size:xx-large">Uneaten Burgers</strong></h4>
    </div>
    <ul class="collection black-text"style="font-size:medium">
      {{#each burgers}} {{#unless this.devoured}} {{> burgers/burger-block devoured=false}} {{/unless}} {{/each}}
    </ul>
  </div>
  <div class="col s6">
    <div class="transparent card-panel">
      <h4><strong class="black-text text-darken-4 flow-text"style="font-size:xx-large">Eaten Burgers</strong></h4>
    </div>
    <ul class="collection black-text"style="font-size:medium">
      {{#each burgers}} {{#if this.devoured}} {{> burgers/burger-block devoured=true}} {{/if}} {{/each}}
    </ul>
  </div>
</div>